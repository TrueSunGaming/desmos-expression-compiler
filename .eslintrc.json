{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["warn", 4],
        "linebreak-style": ["error", "windows"],
        "quotes": ["warn", "double"],
        "semi": ["warn", "always"],
        "no-unused-vars": "warn",
        "no-empty-function": "off"
    }
}
